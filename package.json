{
	"name": "1000pagosback",
	"version": "1.0.0",
	"description": "",
	"main": "dist/app.ts",
	"scripts": {
		"kill:office": "sudo kill -9 $(sudo lsof -t -i:5051)",
		"kill:files": "sudo kill -9 $(sudo lsof -t -i:6060)",
		"kill:socket": "sudo kill -9 $(sudo lsof -t -i:777)",
		"kill:providers": "sudo kill -9 $(sudo lsof -t -i:8000)",
		"start:office": "ts-node src/app.ts",
		"serve:office": "nodemon src/app.ts",
		"qa:office": "ts-node cli/qa",
		"start:files": "ts-node src/app.ts",
		"serve:files": "nodemon src/app.ts",
		"qa:files": "ts-node cli/qa",
		"start:socket": "ts-node src/services/socket/index.ts",
		"serve:socket": "nodemon src/services/socket/index.ts",
		"qa:socket": "ts-node cli/qa",
		"start:providers": "ts-node src/services/Integration_providers/index.ts",
		"serve:providers": "nodemon src/services/Integration_providers/index.ts",
		"qa:providers": "ts-node cli/qa",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"build": "tsc",
		"table": "yarn typeorm entity:create -n",
		"migration:preData": "ts-node src/db/contents/index.ts",
		"migration:run": "yarn typeorm migration:run",
		"migration:create": "yarn typeorm migration:generate -- -n",
		"migration:revert": "yarn typeorm migration:revert",
		"migration:show": "yarn typeorm migration:show"
	},
	"author": "Dimas, Jesus, Aldrin, Carlos, Armando",
	"license": "ISC",
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.9",
		"@types/express": "^4.17.11",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/luxon": "^2.0.7",
		"@types/multer": "^1.4.5",
		"@types/node": "^16.9.1",
		"@types/node-cron": "^2.0.3",
		"@types/nodemailer": "^6.4.1",
		"@types/uuid": "^8.3.1",
		"@types/validator": "^13.6.3",
		"tsc-watch": "^4.2.9",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"@types/shelljs": "^0.8.9",
		"@types/yaml": "^1.9.7",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"child_process": "^1.0.2",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dotenv": "^8.6.0",
		"express": "^4.17.1",
		"express-validator": "^6.12.1",
		"jimp": "^0.16.1",
		"jsonwebtoken": "^8.5.1",
		"luxon": "^2.1.1",
		"mssql": "^7.2.1",
		"multer": "^1.4.2",
		"node-cron": "^2.0.3",
		"nodemon": "^2.0.14",
		"nodemailer-express-handlebars": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"nodemailer": "^6.7.5",
		"shelljs": "^0.8.4",
		"socket.io": "^4.2.0",
		"ts-node": "^10.4.0",
		"typeorm": "^0.2.37",
		"uuid": "^8.3.2"
	},
	"repository": "https://github.com/leomerida15/1000Pagos-Back.git"
}
